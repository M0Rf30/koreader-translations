msgstr "전체"

#: frontend/ui/data/koptoptions.lua:127
msgid "width"
msgstr "폭"

#: frontend/ui/data/koptoptions.lua:127
msgid "height"
msgstr "높이"

#: frontend/ui/data/koptoptions.lua:135
